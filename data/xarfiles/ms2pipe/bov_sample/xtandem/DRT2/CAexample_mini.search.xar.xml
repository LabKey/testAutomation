<?xml version="1.0" encoding="UTF-8"?>
<exp:ExperimentArchive xmlns:exp="http://cpas.fhcrc.org/exp/xml" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                       xsi:schemaLocation="http://cpas.fhcrc.org/exp/xml http://www.labkey.org/download/XarSchema/V2.2/expTypes.xsd">
    <exp:Experiment rdf:about="${FolderLSIDBase}:DefaultExperiment">
        <exp:Name>Default Experiment</exp:Name>
        <exp:Hypothesis/>
    </exp:Experiment>
	<exp:ProtocolDefinitions>
		<exp:Protocol rdf:about="${FolderLSIDBase}:MS2.XTandemSearch:2">
			<exp:Name>XTandem analysis</exp:Name>
			<exp:ApplicationType>ProtocolApplication</exp:ApplicationType>
			<exp:MaxInputMaterialPerInstance>0</exp:MaxInputMaterialPerInstance>
			<exp:MaxInputDataPerInstance xsi:nil="true"/>
			<exp:OutputMaterialPerInstance>0</exp:OutputMaterialPerInstance>
			<exp:OutputDataPerInstance>1</exp:OutputDataPerInstance>
			<exp:ParameterDeclarations>
				<exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String"/>
				<exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String"/>
				<exp:SimpleVal Name="OutputDataLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataLSID" ValueType="String"/>
				<exp:SimpleVal Name="OutputDataNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataName" ValueType="String"/>
				<exp:SimpleVal Name="OutputDataFileTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataFile" ValueType="String"/>
            </exp:ParameterDeclarations>
			<exp:Properties/>
		</exp:Protocol>
		<exp:Protocol rdf:about="${FolderLSIDBase}:MS2.ConvertToPepXml:2">
			<exp:Name>Convert To PepXml</exp:Name>
			<exp:ApplicationType>ProtocolApplication</exp:ApplicationType>
			<exp:MaxInputMaterialPerInstance>0</exp:MaxInputMaterialPerInstance>
			<exp:MaxInputDataPerInstance>1</exp:MaxInputDataPerInstance>
			<exp:OutputMaterialPerInstance>0</exp:OutputMaterialPerInstance>
			<exp:OutputDataPerInstance>1</exp:OutputDataPerInstance>
			<exp:ParameterDeclarations>
                <exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String"/>
                <exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String"/>
                <exp:SimpleVal Name="OutputDataLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataLSID" ValueType="String"/>
				<exp:SimpleVal Name="OutputDataNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataName" ValueType="String"/>
                <exp:SimpleVal Name="OutputDataFileTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataFile" ValueType="String"/>
			</exp:ParameterDeclarations>
			<exp:Properties/>
		</exp:Protocol>
        <exp:Protocol rdf:about="${FolderLSIDBase}:MS2.XInteract.PeptideProphet:2">
            <exp:Name>Run Peptide Prophet using XInteract</exp:Name>
            <exp:ApplicationType>ProtocolApplication</exp:ApplicationType>
            <exp:MaxInputMaterialPerInstance>0</exp:MaxInputMaterialPerInstance>
            <exp:MaxInputDataPerInstance xsi:nil="true"/>
            <exp:OutputMaterialPerInstance>0</exp:OutputMaterialPerInstance>
            <exp:OutputDataPerInstance>1</exp:OutputDataPerInstance>
            <exp:ParameterDeclarations>
                <exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String"/>
                <exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String"/>
                <exp:SimpleVal Name="OutputDataLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataLSID" ValueType="String"/>
                <exp:SimpleVal Name="OutputDataNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataName" ValueType="String"/>
                <exp:SimpleVal Name="OutputDataFileTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataFile" ValueType="String"/>
            </exp:ParameterDeclarations>
            <exp:Properties/>
        </exp:Protocol>
        <exp:Protocol rdf:about="${FolderLSIDBase}:MS2.XInteract.ProteinProphet:2">
            <exp:Name>Run Protein Prophet using XInteract</exp:Name>
            <exp:ApplicationType>ProtocolApplication</exp:ApplicationType>
            <exp:MaxInputMaterialPerInstance>0</exp:MaxInputMaterialPerInstance>
            <exp:MaxInputDataPerInstance>1</exp:MaxInputDataPerInstance>
            <exp:OutputMaterialPerInstance>0</exp:OutputMaterialPerInstance>
            <exp:OutputDataPerInstance>1</exp:OutputDataPerInstance>
            <exp:ParameterDeclarations>
                <exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String"/>
                <exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String"/>
                <exp:SimpleVal Name="OutputDataLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataLSID" ValueType="String"/>
                <exp:SimpleVal Name="OutputDataNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataName" ValueType="String"/>
                <exp:SimpleVal Name="OutputDataFileTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataFile" ValueType="String"/>
            </exp:ParameterDeclarations>
            <exp:Properties/>
        </exp:Protocol>
		<exp:Protocol rdf:about="${FolderLSIDBase}:MS2.XTandemAnalysis:3">
			<exp:Name>X!Tandem Analysis</exp:Name>
			<exp:ProtocolDescription>Use X!Tandem to search for peptides in one mzXml file</exp:ProtocolDescription>
			<exp:ApplicationType>ExperimentRun</exp:ApplicationType>
			<exp:MaxInputMaterialPerInstance xsi:nil="true"/>
			<exp:MaxInputDataPerInstance xsi:nil="true"/>
			<exp:OutputMaterialPerInstance xsi:nil="true"/>
			<exp:OutputDataPerInstance xsi:nil="true"/>
			<exp:ParameterDeclarations>
				<exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String">${RunLSIDBase}:MS2.XTandemAnalysis</exp:SimpleVal>
				<exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String">XTandem analysis run</exp:SimpleVal>
			</exp:ParameterDeclarations>
            <exp:Properties>
                <exp:SimpleVal Name="Category" OntologyEntryURI="terms.fhcrc.org#RunProtocolTypes.Category"
                               ValueType="String">xtandem</exp:SimpleVal>
            </exp:Properties>
		</exp:Protocol>
        <exp:Protocol rdf:about="${FolderLSIDBase}:MarkRunOutput:2">
            <exp:Name>Mark Run Outputs</exp:Name>
            <exp:ProtocolDescription/>
            <exp:ApplicationType>ExperimentRunOutput</exp:ApplicationType>
            <exp:MaxInputMaterialPerInstance xsi:nil="true"/>
            <exp:MaxInputDataPerInstance xsi:nil="true"/>
            <exp:OutputMaterialPerInstance>0</exp:OutputMaterialPerInstance>
            <exp:OutputDataPerInstance>0</exp:OutputDataPerInstance>
            <exp:ParameterDeclarations>
                <exp:SimpleVal Name="ApplicationLSIDTemplate"
                               OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String">${RunLSIDBase}:MarkOutput</exp:SimpleVal>
                <exp:SimpleVal Name="ApplicationNameTemplate"
                               OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String">Run Output</exp:SimpleVal>
            </exp:ParameterDeclarations>
        </exp:Protocol>
	</exp:ProtocolDefinitions>
	<exp:ProtocolActionDefinitions>
		<exp:ProtocolActionSet ParentProtocolLSID="${FolderLSIDBase}:MS2.XTandemAnalysis:3">
			<exp:ProtocolAction ChildProtocolLSID="${FolderLSIDBase}:MS2.XTandemAnalysis:3" ActionSequence="1">
				<exp:PredecessorAction ActionSequenceRef="1"/>
			</exp:ProtocolAction>
			<exp:ProtocolAction ChildProtocolLSID="${FolderLSIDBase}:MS2.XTandemSearch:2" ActionSequence="50">
				<exp:PredecessorAction ActionSequenceRef="1"/>
			</exp:ProtocolAction>
			<exp:ProtocolAction ChildProtocolLSID="${FolderLSIDBase}:MS2.ConvertToPepXml:2" ActionSequence="60">
				<exp:PredecessorAction ActionSequenceRef="50"/>
			</exp:ProtocolAction>
            <exp:ProtocolAction ChildProtocolLSID="${FolderLSIDBase}:MS2.XInteract.PeptideProphet:2" ActionSequence="70">
                <exp:PredecessorAction ActionSequenceRef="60"/>
            </exp:ProtocolAction>
            <exp:ProtocolAction ChildProtocolLSID="${FolderLSIDBase}:MS2.XInteract.ProteinProphet:2" ActionSequence="80">
                <exp:PredecessorAction ActionSequenceRef="70"/>
            </exp:ProtocolAction>
            <exp:ProtocolAction ChildProtocolLSID="${FolderLSIDBase}:MarkRunOutput:2" ActionSequence="1000">
                <exp:PredecessorAction ActionSequenceRef="80"/>
            </exp:ProtocolAction>
		</exp:ProtocolActionSet>
	</exp:ProtocolActionDefinitions>
	<exp:StartingInputDefinitions>
		<exp:Data rdf:about="${AutoFileLSID}">
			<exp:Name>CAexample_mini.mzXML</exp:Name>
			<exp:CpasType>Data</exp:CpasType>
			<exp:DataFileUrl>../../CAexample_mini.mzXML</exp:DataFileUrl>
		</exp:Data>

		<exp:Data rdf:about="${AutoFileLSID}">
			<exp:Name>Tandem Settings</exp:Name>
			<exp:CpasType>Data</exp:CpasType>
			<exp:DataFileUrl>tandem.xml</exp:DataFileUrl>
		</exp:Data>
		<exp:Data rdf:about="${AutoFileLSID}">
			<exp:Name>Bovine_mini1.fasta</exp:Name>
			<exp:CpasType>Data</exp:CpasType>
			<exp:DataFileUrl>../../../databases/Bovine_mini1.fasta</exp:DataFileUrl>
		</exp:Data>
        <exp:Data rdf:about="${AutoFileLSID}">
            <exp:Name>Bovine_mini2.fasta</exp:Name>
            <exp:CpasType>Data</exp:CpasType>
            <exp:DataFileUrl>../../../databases/Bovine_mini2.fasta</exp:DataFileUrl>
        </exp:Data>
        <exp:Data rdf:about="${AutoFileLSID}">
            <exp:Name>Bovine_mini3.fasta</exp:Name>
            <exp:CpasType>Data</exp:CpasType>
            <exp:DataFileUrl>../../../databases/Bovine_mini3.fasta</exp:DataFileUrl>
        </exp:Data>

	</exp:StartingInputDefinitions>
	<exp:ExperimentRuns>
		<exp:ExperimentRun rdf:about="${FolderLSIDBase}:MS2-drt/xtandem/DRT2/CAexample_mini">
			<exp:Name>drt/CAexample_mini (DRT2)</exp:Name>
			<exp:ProtocolLSID>${FolderLSIDBase}:MS2.XTandemAnalysis:3</exp:ProtocolLSID>
			<exp:Comments/>
			<exp:Properties/>
			<exp:ExperimentLog>
                <exp:ExperimentLogEntry ActionSequenceRef="1">
                    <exp:ApplicationInstanceCollection>
                        <exp:InstanceDetails>
                            <exp:InstanceInputs>
                                <exp:DataLSID DataFileUrl="tandem.xml" RoleName="SearchConfig">${AutoFileLSID}</exp:DataLSID>
                                <exp:DataLSID DataFileUrl="../../../databases/Bovine_mini1.fasta" RoleName="FASTA">${AutoFileLSID}</exp:DataLSID>
                                <exp:DataLSID DataFileUrl="../../../databases/Bovine_mini2.fasta" RoleName="FASTA">${AutoFileLSID}</exp:DataLSID>
                                <exp:DataLSID DataFileUrl="../../../databases/Bovine_mini3.fasta" RoleName="FASTA">${AutoFileLSID}</exp:DataLSID>

                                <exp:DataLSID DataFileUrl="../../CAexample_mini.mzXML" RoleName="mzXML">${AutoFileLSID}</exp:DataLSID>

                            </exp:InstanceInputs>
                        </exp:InstanceDetails>
                    </exp:ApplicationInstanceCollection>
                </exp:ExperimentLogEntry>
                <exp:ExperimentLogEntry ActionSequenceRef="50">
                    <exp:CommonParametersApplied>
                        <exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String">${RunLSIDBase}:MS2.XTandemSearch</exp:SimpleVal>
                        <exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String">X!Tandem Database Search</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataLSID" ValueType="String">${RunLSIDBase}:XTanFile</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataName" ValueType="String">XTandem Search Results</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataFileTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataFile" ValueType="String">CAexample_mini.xtan.xml</exp:SimpleVal>
                    </exp:CommonParametersApplied>
                  </exp:ExperimentLogEntry>
                <exp:ExperimentLogEntry ActionSequenceRef="60">
                    <exp:CommonParametersApplied>
                        <exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String">${RunLSIDBase}:MS2.ConvertToPepXml</exp:SimpleVal>
                        <exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String">Convert to PepXml</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataLSID" ValueType="String">${RunLSIDBase}:PepXmlFile</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataName" ValueType="String">PepXml/XTandem Search Results</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataFileTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataFile" ValueType="String">CAexample_mini.pep.xml</exp:SimpleVal>
                    </exp:CommonParametersApplied>
                  </exp:ExperimentLogEntry>
                <exp:ExperimentLogEntry ActionSequenceRef="70">
                    <exp:CommonParametersApplied>
                        <exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String">${RunLSIDBase}:MS2.XInteract.PeptideProphet</exp:SimpleVal>
                        <exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String">Peptide Prophet</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataLSID" ValueType="String">${RunLSIDBase}:ScoredPepXmlFile</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataName" ValueType="String">Scored Search Results</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataFileTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataFile" ValueType="String">CAexample_mini.pep.xml</exp:SimpleVal>
                    </exp:CommonParametersApplied>
                  </exp:ExperimentLogEntry>
                <exp:ExperimentLogEntry ActionSequenceRef="80">
                    <exp:CommonParametersApplied>
                        <exp:SimpleVal Name="ApplicationLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationLSID" ValueType="String">${RunLSIDBase}:MS2.XInteract.ProteinProphet</exp:SimpleVal>
                        <exp:SimpleVal Name="ApplicationNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.ApplicationName" ValueType="String">Protein Prophet</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataLSIDTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataLSID" ValueType="String">${RunLSIDBase}:ProteinScoresFile</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataNameTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataName" ValueType="String">Protein Prophet scores</exp:SimpleVal>
                        <exp:SimpleVal Name="OutputDataFileTemplate" OntologyEntryURI="terms.fhcrc.org#XarTemplate.OutputDataFile" ValueType="String">CAexample_mini.prot.xml</exp:SimpleVal>
                    </exp:CommonParametersApplied>
                  </exp:ExperimentLogEntry>

				<exp:ExperimentLogEntry ActionSequenceRef="1000">
                    <exp:ApplicationInstanceCollection>
                        <exp:InstanceDetails>
                            <exp:InstanceInputs>
                                <exp:DataLSID>${RunLSIDBase}:ProteinScoresFile</exp:DataLSID>
                                <exp:DataLSID>${RunLSIDBase}:ScoredPepXmlFile</exp:DataLSID>
                            </exp:InstanceInputs>
                        </exp:InstanceDetails>
                    </exp:ApplicationInstanceCollection>
                </exp:ExperimentLogEntry>
            </exp:ExperimentLog>
			<exp:ProtocolApplications/>
		</exp:ExperimentRun>
	</exp:ExperimentRuns>
</exp:ExperimentArchive>
