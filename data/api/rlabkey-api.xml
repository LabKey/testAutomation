<ApiTests xmlns="http://labkey.org/query/xml">
    <test name="Rlabkey version check" type="debug">
        <url>
            <![CDATA[
                library(Rlabkey)
                cat("Found Rlabkey\nVersion:", installed.packages()["Rlabkey","Version"], '\n')
            ]]>
        </url>
        <response>
            <![CDATA[
                Found Rlabkey
            ]]>
        </response>
    </test>
    <test name="ls folders" type="post">
        <url>
            <![CDATA[
                library(Rlabkey)
                print(lsFolders(getSession(baseUrl=labkey.url.base, "/%projectName%")))
            ]]>
        </url>
        <response>
            <![CDATA[
                "/%projectName%"
                "/%projectName%/RlabkeyTest"
            ]]>
        </response>
    </test>
    <test name="get folders" type="post">
        <url>
            <![CDATA[
                library(Rlabkey)
                print(labkey.getFolders(labkey.url.base, "/%projectName%", includeEffectivePermissions=FALSE))
            ]]>
        </url>
        <response>
            <![CDATA[
                %projectName% /%projectName%
                RlabkeyTest /%projectName%/RlabkeyTest
            ]]>
        </response>
    </test>
</ApiTests>
